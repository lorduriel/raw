{
    "name": "lordfm/raw",
    "description": "Repository pattern boilerplate based on Annotated Models",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "lordfm",
            "email": "urieluvd@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "doctrine/annotations": "^1.4",
        "symfony/class-loader": "2.8",
        "icanboogie/inflector": "1.4.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "..\\..\\app",
            "LoRDFM\\Raw\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "..\\..\\app",
            "LoRDFM\\Raw\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "orchestra/testbench": "^3.4"
    }
}
